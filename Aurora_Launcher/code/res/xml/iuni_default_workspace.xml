<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.aurora.launcher">
    <!-- First screen [1] -->
    <appwidget
        launcher:packageName="com.android.deskclock"
        launcher:className="com.aurora.alarmclock.DigitalAppWidgetProvider"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="0"
        launcher:spanX="4"
        launcher:spanY="3" />
    <favorite
        launcher:packageName="com.android.camera"
        launcher:className="com.android.camera.CameraLauncher"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="3" />
    <favorite
        launcher:packageName="com.android.gallery3d"
        launcher:className="com.android.gallery3d.app.Gallery"
        launcher:screen="0"
        launcher:x="1"
        launcher:y="3" />
    <favorite
        launcher:packageName="com.aurora.filemanager"
        launcher:className="com.aurora.filemanager.FileExplorerTabActivity"
        launcher:screen="0"
        launcher:x="2"
        launcher:y="3" />
    <favorite
        launcher:packageName="com.aurora.secure"
        launcher:className="com.secure.activity.MainActivity"
        launcher:screen="0"
        launcher:x="3"
        launcher:y="3" />
    
     <favorite
        launcher:packageName="com.android.deskclock"
        launcher:className="com.android.deskclock.AlarmClock"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="4" />
    <favorite
        launcher:packageName="com.android.calendar"
        launcher:className="com.android.calendar.AllInOneActivity"
        launcher:screen="0"
        launcher:x="1"
        launcher:y="4" />
    <favorite
        launcher:packageName="com.android.settings"
        launcher:className="com.android.settings.Settings"
        launcher:screen="0"
        launcher:x="2"
        launcher:y="4" />
    <favorite
        launcher:packageName="gn.com.android.update"
        launcher:className="gn.com.android.update.ui.UpdateUiActivity"
        launcher:screen="0"
        launcher:x="3"
        launcher:y="4" />
    
    <!-- Second screen [2] -->
        <favorite
        launcher:packageName="com.aurora.note"
        launcher:className="com.aurora.note.NoteMainActivity"
        launcher:screen="1"
        launcher:x="0"
        launcher:y="0" />
    <favorite
        launcher:packageName="com.aurora.netmanage"
        launcher:className="com.netmanage.activity.NetMainActivity"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="0" />
    <favorite
        launcher:packageName="com.android.calculator2"
        launcher:className="com.android.calculator2.Calculator"
        launcher:screen="1"
        launcher:x="2"
        launcher:y="0" />

    <!-- Hotseat (We use the screen as the position of the item in the hotseat) -->
    <favorite_dock
        launcher:container="-101"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="0"
        launcher:queryIntent="\#Intent;action=android.intent.action.DIAL;launchFlags=0x10000000;end"/>
    <favorite_dock
        launcher:container="-101"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="0"
        launcher:queryIntent="" />
    <favorite_dock
        launcher:container="-101"
        launcher:screen="2"
        launcher:x="2"
        launcher:y="0"
        launcher:queryIntent="\#Intent;action=android.intent.action.MAIN;type=vnd.android-dir/mms-sms;launchFlags=0x10000000;end"/>
    <favorite_dock
        launcher:container="-101"
        launcher:screen="3"
        launcher:x="3"
        launcher:y="0" 
        launcher:queryIntent="http://www.google.com#Intent;action=android.intent.action.VIEW;launchFlags=0x10000000;end" /> 
</favorites>
