<aurora.preference.AuroraPreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:aurora="http://schemas.android.com/apk/res/com.aurora"
        xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
        aurora:auroratitle="@string/apn_edit">
        <!--  
    <aurora.preference.AuroraEditTextPreference
        aurora:auroratitle="@string/apn_name"
        aurora:auroradialogTitle="@string/apn_name"
        aurora:aurorakey="apn_name"
        android:singleLine="true"
        android:inputType="text"
        />
     -->   
      <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_name"
        aurora:auroradialogTitle="@string/apn_name"
        aurora:aurorakey="apn_name"
        android:singleLine="true"
        android:inputType="text"
        />  
        
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_apn"
        aurora:auroradialogTitle="@string/apn_apn"
        aurora:aurorakey="apn_apn"
        android:singleLine="true"
        android:inputType="textEmailAddress"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_http_proxy"
        aurora:auroradialogTitle="@string/apn_http_proxy"
        aurora:aurorakey="apn_http_proxy"
        android:singleLine="true"
        android:inputType="textUri"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_http_port"
        aurora:auroradialogTitle="@string/apn_http_port"
        aurora:aurorakey="apn_http_port"
        android:singleLine="true"
        android:inputType="number"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_user"
        aurora:auroradialogTitle="@string/apn_user"
        aurora:aurorakey="apn_user"
        android:singleLine="true"
        android:inputType="textEmailAddress"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_password"
        aurora:auroradialogTitle="@string/apn_password"
        aurora:aurorakey="apn_password"
        android:singleLine="true"
        android:inputType="textPassword"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_server"
        aurora:auroradialogTitle="@string/apn_server"
        aurora:aurorakey="apn_server"
        android:singleLine="true"
        android:inputType="textUri"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_mmsc"
        aurora:auroradialogTitle="@string/apn_mmsc"
        aurora:aurorakey="apn_mmsc"
        android:singleLine="true"
        android:inputType="textUri"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_mms_proxy"
        aurora:auroradialogTitle="@string/apn_mms_proxy"
        aurora:aurorakey="apn_mms_proxy"
        android:singleLine="true"
        android:inputType="textUri"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_mms_port"
        aurora:auroradialogTitle="@string/apn_mms_port"
        aurora:aurorakey="apn_mms_port"
        android:singleLine="true"
        android:inputType="number"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_mcc"
        aurora:auroradialogTitle="@string/apn_mcc"
        aurora:aurorakey="apn_mcc"
        android:singleLine="true"
        android:inputType="number"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_mnc"
        aurora:auroradialogTitle="@string/apn_mnc"
        aurora:aurorakey="apn_mnc"
        android:singleLine="true"
        android:inputType="number"
        />
    <aurora.preference.AuroraListPreference
        aurora:auroratitle="@string/apn_auth_type"
        aurora:aurorakey="auth_type"
        aurora:auroraentries="@array/apn_auth_entries"
        aurora:auroraentryValues="@array/apn_auth_values"
        />
    <com.android.settings.AuroraApnEditTextPreference
        aurora:auroratitle="@string/apn_type"
        aurora:auroradialogTitle="@string/apn_type"
        aurora:aurorakey="apn_type"
        android:singleLine="true"
        android:inputType="textNoSuggestions"
        />
    <aurora.preference.AuroraListPreference
        aurora:auroratitle="@string/apn_protocol"
        aurora:auroradialogTitle="@string/apn_protocol"
        aurora:aurorakey="apn_protocol"
        aurora:auroraentries="@array/apn_protocol_entries"
        aurora:auroraentryValues="@array/apn_protocol_values"
        />
    <aurora.preference.AuroraListPreference
        aurora:auroratitle="@string/apn_roaming_protocol"
        aurora:auroradialogTitle="@string/apn_roaming_protocol"
        aurora:aurorakey="apn_roaming_protocol"
        aurora:auroraentries="@array/apn_protocol_entries"
        aurora:auroraentryValues="@array/apn_protocol_values"
        />
    <aurora.preference.AuroraCheckBoxPreference
        aurora:auroratitle="@string/carrier_enabled"
        aurora:aurorakey="carrier_enabled"
        aurora:auroraenabled="false"
        aurora:aurorasummaryOn="@string/carrier_enabled_summaryOn"
        aurora:aurorasummaryOff="@@string/carrier_enabled_summaryOff"
        />
    <aurora.preference.AuroraListPreference
        aurora:auroratitle="@string/bearer"
        aurora:aurorakey="bearer"
        aurora:auroraentries="@array/bearer_entries"
        aurora:auroraentryValues="@array/bearer_values"
        />
</aurora.preference.AuroraPreferenceScreen>   
