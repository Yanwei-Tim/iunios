<aurora.preference.AuroraPreferenceScreen
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:aurora="http://schemas.android.com/apk/res/com.aurora"
        aurora:auroratitle="@string/bluetooth_device_advanced_title">
	
    <!--  Aurora liugj 2013-10-22 modified for aurora's new feature start -->
    <aurora.preference.AuroraPreferenceScreen
            aurora:aurorakey="rename_device"
            aurora:auroratitle="@string/bluetooth_device_advanced_rename_device"
            aurora:auroraorder="20"
            aurora:aurorapersistent="false" />
    <!--  Aurora liugj 2013-10-22 modified for aurora's new feature end -->

    <aurora.preference.AuroraPreference
            aurora:aurorakey="unpair"
            aurora:auroratitle="@string/bluetooth_device_context_unpair"
            aurora:auroraorder="40"
            aurora:aurorapersistent="false"
            />

    <aurora.preference.AuroraPreferenceCategory
            aurora:aurorakey="profile_container"
            aurora:auroraorder="100"
            aurora:auroratitle="@string/bluetooth_device_advanced_profile_header_title">

        <!-- Profile checkboxes will be added here programmatically. -->
        
    </aurora.preference.AuroraPreferenceCategory>

</aurora.preference.AuroraPreferenceScreen>
