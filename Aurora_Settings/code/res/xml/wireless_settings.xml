<aurora.preference.AuroraPreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:aurora="http://schemas.android.com/apk/res/com.aurora"
        aurora:auroratitle="@string/wireless_networks_settings_title">

    <aurora.preference.AuroraCheckBoxPreference
        aurora:aurorakey="toggle_airplane"
        aurora:auroratitle="@string/airplane_mode"
        aurora:aurorapersistent="false"
        aurora:auroradisableDependentsState="true" />

    <!-- Gionee:zhang_xin 2013-02-01 add for CR00767874 start -->
    <!-- Data Usage -->
    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorakey="data_usage_settings"
        aurora:auroratitle="@string/data_usage_summary_title" >
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.gionee.settings.adapter"
            android:targetClass="com.gionee.settings.adapter.DataUsageSummaryActivity" />
    </aurora.preference.AuroraPreferenceScreen>
    <!-- Gionee:zhang_xin 2013-02-01 add for CR00767874 end -->

    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorafragment="com.android.settings.vpn2.VpnSettings"
        aurora:aurorakey="vpn_settings"
        aurora:auroratitle="@string/vpn_settings_title" />

    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorafragment="com.android.settings.TetherSettings"
        aurora:aurorakey="tether_settings"
        aurora:auroratitle="@string/tether_settings_title_all" />

    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorakey="wimax_settings"
        aurora:auroratitle="@string/wimax_settings"
        aurora:aurorasummary="@string/wimax_settings_summary" >
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.settings.wimax"
            android:targetClass="com.android.settings.wimax.WimaxSettings" />
    </aurora.preference.AuroraPreferenceScreen>

    <aurora.preference.AuroraSwitchPreference
        aurora:aurorakey="toggle_nfc"
        aurora:auroratitle="@string/nfc_quick_toggle_title"
        aurora:aurorapersistent="false" />
<!-- aurora:aurorasummary="@string/nfc_quick_toggle_summary" -->

    <!--<com.mediatek.nfc.NfcPreference
        android:key="toggle_mtk_nfc"
        android:title="@string/nfc_quick_toggle_title"
        android:persistent="false" />-->
        
    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorafragment="com.android.settings.nfc.AndroidBeam"
        aurora:aurorakey="android_beam_settings"
        aurora:auroratitle="@string/android_beam_settings_title" />

    <!-- Gionee liuyanbo 2012-11-19 add for CR00734108 start -->
    <aurora.preference.AuroraCheckBoxPreference
        aurora:aurorakey="toggle_wifi_p2p"
        aurora:auroratitle="@string/wifi_p2p_settings_title"
        aurora:aurorapersistent="false" />
    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorafragment="com.android.settings.wifi.p2p.WifiP2pSettings"
        aurora:aurorakey="wifi_p2p_settings"
        aurora:auroratitle="@string/wifi_p2p_settings_title"
        aurora:aurorasummary="@string/wifi_p2p_settings_title">
    </aurora.preference.AuroraPreferenceScreen>
    <!-- Gionee liuyanbo 2012-11-19 add for CR00734108 end -->
        
    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorakey="rcse_settings"
        aurora:auroratitle="@string/rcse_settings_title">
    </aurora.preference.AuroraPreferenceScreen>

    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorakey="mobile_network_settings"
        aurora:auroratitle="@string/network_settings_title"
        >
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.phone"
            android:targetClass="com.android.phone.MobileNetworkSettings" />
    </aurora.preference.AuroraPreferenceScreen>

    <aurora.preference.AuroraCheckBoxPreference
        aurora:aurorakey="toggle_nsd"
        aurora:auroratitle="@string/nsd_quick_toggle_title"
        aurora:aurorasummary="@string/nsd_quick_toggle_summary"
        aurora:aurorapersistent="false" />

    <aurora.preference.AuroraCheckBoxPreference
        aurora:aurorakey="usb_sharing"
        aurora:auroratitle="@string/usb_sharing_title"
        aurora:aurorasummary="@string/usb_sharing_summary" 
        aurora:aurorapersistent="false" />

    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorafragment="com.android.settings.ProxySelector"
        aurora:aurorakey="proxy_settings"
        aurora:auroratitle="@string/proxy_settings_title" />

    <aurora.preference.AuroraPreferenceScreen
        aurora:aurorakey="cell_broadcast_settings"
        aurora:auroratitle="@string/cell_broadcast_settings"
        aurora:aurorasummary="@string/cell_broadcast_settings_summary" >
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.cellbroadcastreceiver"
            android:targetClass="com.android.cellbroadcastreceiver.CellBroadcastSettings" />
    </aurora.preference.AuroraPreferenceScreen>

</aurora.preference.AuroraPreferenceScreen>
