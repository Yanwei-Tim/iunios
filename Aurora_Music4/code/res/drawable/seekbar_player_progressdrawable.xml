<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <inset
            android:drawable="@drawable/default_myseekbar_bg"
            android:insetRight="3dp"
            android:insetLeft="3dp"
            android:insetBottom="3dp"
            android:insetTop="3dp" />
    </item>
    <item android:id="@android:id/secondaryProgress"
        >
        <clip>
            <shape>
                <corners android:radius="4dp" />
                <stroke android:width="6dp" />
                <padding
                    android:bottom="3dp"
                    android:left="3dp"
                    android:right="3dp"
                    android:top="3dp" />

                <gradient
                    android:angle="0"
                    android:centerY="0.3"
                    android:endColor="#ffd2d6e1"
                    android:startColor="#ffd2d6e1" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="4dp" />
                <stroke android:width="6dp" />

                <padding
                    android:bottom="3dp"
                    android:left="3dp"
                    android:right="3dp"
                    android:top="3dp" />

                <gradient
                    android:angle="0"
                    android:centerColor="@color/aurora_progress_center"
                    android:centerY="0.3"
                    android:centerX="0.3"
                    android:endColor="@color/aurora_progress_end"
                    android:startColor="@color/aurora_progress_start" />
            </shape>
        </clip>
    </item>

</layer-list>