<?xml version="1.0" encoding="utf-8"?>
<!-- Gionee <jiating> <2013-04-24> modify for CR00000000 AllInOneActivity actionBar View begin -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="56dp"
    android:background="@drawable/gn_all_in_one_title_bg" >

    <RelativeLayout
        android:id="@+id/gn_all_in_one_actionbar_time_operation"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginBottom="2dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/gn_all_in_one_action_bar_select_date_on_off"
        android:paddingLeft="10dp" >

        <TextView
            android:id="@+id/gn_all_in_one_actionbar_time_year_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:textColor="#888888"
            android:textSize="@dimen/gn_all_in_one_action_select_Date_size" />

        <TextView
            android:id="@+id/gn_all_in_one_actionbar_time_month_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/gn_all_in_one_actionbar_time_year_show"
            android:gravity="center_vertical"
            android:textColor="#888888"
            android:textSize="@dimen/gn_all_in_one_action_select_Date_size" />

        <ImageView
            android:id="@+id/gn_all_in_one_actionbar_time_operation_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/gn_all_in_one_actionbar_time_month_show"
            android:src="@drawable/gn_all_in_one_action_bar_time_sift"
            android:visibility="gone" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/gn_all_in_one_create_event"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginBottom="2dp"
        android:layout_marginRight="14dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/gn_all_in_one_create_button_on_off"
        android:gravity="center_horizontal" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@drawable/gn_all_in_one_create_view_on_off"
            android:paddingLeft="9dp"
            android:paddingRight="9dp" />
    </RelativeLayout>

    <aurora.widget.AuroraButton
        android:id="@+id/gn_all_in_one_actionbar_return_today_view"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_centerVertical="true"
        android:layout_marginRight="14dp"
        android:layout_toLeftOf="@+id/gn_all_in_one_create_event"
        android:background="@drawable/gn_all_in_one_action_bar_today_on_off"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        android:text="@string/gn_all_in_one_action_bar_today"
        android:textColor="@color/gn_all_in_one_actionbar_back_today_text_color"
        android:textSize="@dimen/date_header_text_size" />

</RelativeLayout> <!-- Gionee <jiating> <2013-04-24> modify for CR00000000 AllInOneActivity actionBar View end -->
