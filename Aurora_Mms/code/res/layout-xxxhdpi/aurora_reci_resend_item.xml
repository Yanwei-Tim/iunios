<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="28dip"
    android:background="#ffffff">
    <!-- Aurora xuyong 2013-12-17 modified for aurora's new feature start -->
    <aurora.widget.AuroraCheckBox 
        android:id="@+id/aurora_reci_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginRight="12dip"
        android:layout_marginTop="@com.aurora:dimen/aurora_list_cb_doubleline_margintop"
        android:focusable="false"
        android:clickable="false"
        android:button="@null"/>
    <!-- Aurora xuyong 2013-12-17 modified for aurora's new feature end -->
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="@com.aurora:dimen/aurora_list_doubleline_height"
        android:orientation="vertical"
        android:paddingTop="@com.aurora:dimen/aurora_list_content_layout_paddingtop"
        android:layout_toRightOf="@id/aurora_reci_check">
<!-- Aurora xuyong 2013-12-17 modified for aurora's new feature end -->
        <aurora.widget.AuroraTextView 
            android:id="@+id/aurora_reci_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            style="@com.aurora:style/AuroraListMainTextStyle"/>
        <aurora.widget.AuroraTextView 
            android:id="@+id/aurora_reci_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/aurora_reci_name"
            android:layout_marginTop="@com.aurora:dimen/aurora_list_gap_height_maintext_and_subtext"
            android:singleLine="true"
            style="@com.aurora:style/AuroraListSubTextStyle"/>
<!-- Aurora xuyong 2014-04-03 modified for aurora's new feature end -->
    </RelativeLayout>
<!-- Aurora xuyong 2014-03-17 modified for aurora's new feature end -->
</RelativeLayout>