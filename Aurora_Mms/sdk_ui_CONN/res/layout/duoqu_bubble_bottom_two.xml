<?xml version="1.0" encoding="utf-8"?>
<cn.com.xy.sms.sdk.ui.popu.widget.DuoquRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/duoqu_bubble_bottom_two"
    android:layout_width="match_parent"
    android:layout_height="@dimen/bubble_bottom_two_height">

    <View
        android:id="@+id/duoqu_bottom_split_line"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@drawable/duoqu_line"/>

    <ImageView
        android:id="@+id/duoqu_btn_split_line"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/duoqu_bottom_div_hight"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/duoqu_with_button_div"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/duoqu_btn_2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@id/duoqu_btn_split_line"
        android:layout_centerInParent="true"
        android:visibility="gone" >

        <TextView
            android:id="@+id/duoqu_btn_text_2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="@null"
            android:drawablePadding="2dp"
            android:gravity="center"
            android:textSize="@dimen/duoqu_bubble_bottom_text_size" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/duoqu_btn_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@id/duoqu_btn_split_line"
        android:layout_centerInParent="true"
        android:visibility="gone" >

        <TextView
            android:id="@+id/duoqu_btn_text_1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:textSize="@dimen/duoqu_bubble_bottom_text_size" />
    </FrameLayout>

</cn.com.xy.sms.sdk.ui.popu.widget.DuoquRelativeLayout>