<?xml version="1.0" encoding="utf-8"?>
<cn.com.xy.sms.sdk.ui.popu.widget.DuoquRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/duoqu_card_body_bottom"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true" >

    <TextView
        android:id="@+id/duoqu_call_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="10dp"
        android:textSize="10dp" />

    <ImageView
        android:id="@+id/duoqu_tel_card"
        android:layout_width="9dp"
        android:layout_height="10dp"
        android:layout_centerVertical="true"
        android:layout_marginRight="5dp"
        android:layout_toLeftOf="@id/duoqu_call_name"
        android:gravity="center_horizontal|center_vertical" />

    <TextView
        android:id="@+id/duoqu_call_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginRight="5dp"
        android:layout_toLeftOf="@id/duoqu_tel_card"
        android:textSize="10dp" />

</cn.com.xy.sms.sdk.ui.popu.widget.DuoquRelativeLayout>