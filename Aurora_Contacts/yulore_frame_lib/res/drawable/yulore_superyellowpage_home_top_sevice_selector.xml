<?xml version="1.0" encoding="utf-8"?>
<!-- @color/yulore_superyellowpage_home_top_service_pressed -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@color/yulore_superyellowpage_white" android:state_window_focused="false"/>

    <!-- 非触摸模式下获得焦点并单击 -->
    <item android:drawable="@color/yulore_superyellowpage_home_top_service_pressed" android:state_focused="true" android:state_pressed="true"/>

    <!-- 触摸模式下单击 -->
    <item android:drawable="@color/yulore_superyellowpage_home_top_service_pressed" android:state_focused="false" android:state_pressed="true"/>

    <!-- 选中 -->
    <item android:drawable="@color/yulore_superyellowpage_home_top_service_pressed" android:state_selected="true"/>

    <!-- 获得焦点 -->
    <item android:drawable="@color/yulore_superyellowpage_home_top_service_pressed" android:state_focused="true"/>

    <!-- 默认 -->
    <item android:drawable="@color/yulore_superyellowpage_white"/>
    
</selector>