    <!-- operation stytle start -->
    <style name="OperationButtonStytle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="OperationMoveButtonStytle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">5dip</item>
        <item name="android:layout_marginRight">5dip</item>
        <item name="android:background">@drawable/remote_button_bg</item>
    </style>

    <style name="BottomButtonBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@com.aurora:dimen/aurora_action_bottom_bar_height</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="BottomButtonBar_operation" parent="BottomButtonBar">
        <item name="android:background">@com.aurora:drawable/aurora_bottombarback</item>
    </style>
    <!-- operation stytle end -->

    <style name="aurora_file_name_select">
        <item name="android:maxWidth">@dimen/aurora_file_manager_title_max_size</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="aurora_file_other_select">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#7b7b7b</item>
    </style>

    <style name="aurora_file_name_normal">
        <item name="android:maxWidth">@dimen/aurora_file_manager_title_max_size</item>
        <item name="android:textColor">@color/aurora_operation_more_title_change_color</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="aurora_file_other_normal">
        <item name="android:textColor">@color/aurora_file_item_change_color</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="aurora_file_detail_select">
        <item name="android:textColor">@color/aurora_file_item_change_color</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="aurora_file_detail_normal">
        <item name="android:textColor">@color/aurora_file_item_detail_color</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="AuroraStorageDetailTitleStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/aurora_storage_title_color</item>
        <item name="android:textSize">15sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="AuroraStorageDetailLayoutStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">@dimen/aurora_storage_title_layout_margin_top</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="AuroraStorageDetailLayoutStyle_s" parent="AuroraStorageDetailLayoutStyle">
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="AuroraStorageDetailLayoutStyleSingle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="AuroraStorageCenterTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/aurora_storage_text_color</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="AuroraStorageCenterTextStyle_free" parent="AuroraStorageCenterTextStyle"></style>

    <style name="AuroraStorageCenterTextStyle_sd" parent="AuroraStorageCenterTextStyle"></style>

    <style name="AuroraStorageDetailTextStyleSingle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/aurora_storage_text_color</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="AuroraStorageDetailTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/aurora_storage_text_color</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="BottomButton">
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>

    <style name="BottomButton.Left">
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
    </style>

    <style name="BottomButton.Right">
        <item name="android:layout_marginRight">8dp</item>
    </style>

    <style name="aurora_file_name_select.rlayout">
        <item name="android:layout_marginTop">20dp</item>
    </style>

    <style name="singlepie_rele">
        <item name="android:layout_marginBottom">12dp</item>
    </style>

    <style name="strong_free">
        <item name="android:layout_marginBottom">13dp</item>
    </style>

    <style name="strong_imag_phone">
        <item name="android:src">@drawable/phone_s</item>
    </style>

    <style name="strong_imag_sd">
        <item name="android:src">@drawable/sd_s</item>
    </style>

    <style name="storageDetail">
        <item name="android:layout_marginTop">@dimen/storagedetail_linear</item>
        <item name="android:layout_marginLeft">28dp</item>
    </style>

    <style name="imag_free">
        <item name="android:background">@drawable/aurora_detail_free</item>
    </style>

