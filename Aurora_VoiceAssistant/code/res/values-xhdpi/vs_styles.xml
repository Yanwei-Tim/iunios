	<style name="vs_weather_textview">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:textColor">#ff8d8d8d</item>
	    <item name="android:textSize">10sp</item>
	    <item name="android:gravity">center</item>
	    <item name="android:layout_gravity">center</item>
	    <item name="android:layout_marginTop">5dp</item>
	</style>
	<style name="vs_weather_date">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:textColor">#ff8d8d8d</item>
	    <item name="android:textSize">10sp</item>
	    <item name="android:gravity">center</item>
	    <item name="android:layout_gravity">center</item>
	    <item name="android:layout_marginTop">3dp</item>
	</style>
	<style name="vs_weather_week">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:textColor">#ff8d8d8d</item>
	    <item name="android:textSize">10sp</item>
	    <item name="android:gravity">center</item>
	    <item name="android:layout_gravity">center</item>
	    <item name="android:layout_marginTop">4dp</item>
	    <item name="android:layout_marginBottom">9dp</item>
	</style>
	<style name="vs_weather_temperature">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:gravity">center</item>
	    <item name="android:layout_gravity">center</item>
	    <item name="android:textColor">#ff000000</item>
	    <item name="android:textSize">10sp</item>
	    <item name="android:layout_marginTop">4dp</item>
	    <item name="android:layout_marginBottom">4dp</item>
	</style>
	
	<style name="vs_weather_imageview">
	    <item name="android:layout_width">47dp</item>
	    <item name="android:layout_height">47dp</item>
	    <item name="android:gravity">center</item>
	    <item name="android:layout_gravity">center</item>
	    <item name="android:scaleType">centerInside</item>
	    <item name="android:src">@drawable/vs_weather_xiaoyu_alarm</item>
	</style>
	<style name="vs_weather_divider">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">110dp</item>
	    <item name="android:src">@drawable/vs_weather_6day_vline</item>
	    <item name="android:layout_marginTop">8dp</item>
	    <item name="android:layout_marginBottom">7dp</item>
	</style>
<!--      shigq add -->
    <style name="CutomEditText">
        <item name="android:textSelectHandleLeft">@drawable/aurora_text_select_handle_left</item>
        <item name="android:textSelectHandleRight">@drawable/aurora_text_select_handle_right</item>
        <item name="android:textSelectHandle">@drawable/aurora_text_select_handle_middle</item>
        <item name="android:textCursorDrawable">@drawable/aurora_text_cursor_light</item>
	</style>
	
    <!-- barcode -->
    <style name="vs_barcode_scanning_tips_style">
        <item name="android:text">@string/vs_barcode_scanning_tips</item>
        <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">match_parent</item>
	    <item name="android:layout_marginTop">364.6dp</item>
	    <item name="android:textColor">@color/vs_barcode_scanning_tips_white</item>
	    <item name="android:textSize">@dimen/vs_barcode_scanning_tips_size</item>
	    <item name="android:layout_gravity">top|center_horizontal</item>
    </style>
    <style name="vs_barcode_scanning_getphotos_style">
        <item name="android:text">@string/vs_barcode_getphotos</item>
        <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">29.3dp</item>
	    <item name="android:layout_marginTop">455.6dp</item>
	    <item name="android:textColor">@color/vs_barcode_scanning_tips_white</item>
	    <item name="android:gravity">center</item>
	    <item name="android:layout_gravity">top|center_horizontal</item>
    </style>
    
    <style name="vs_barcode_result_layout_text_style">
        <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:layout_marginLeft">17dp</item>
	    <item name="android:layout_marginRight">17dp</item>
	    <item name="android:textColor">@color/vs_font_style_colors</item>
	    <item name="android:textSize">18sp</item>
	    <item name="android:textStyle">normal</item>
    </style>
    
</resources>
