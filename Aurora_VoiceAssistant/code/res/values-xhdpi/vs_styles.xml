<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
	
    
    <style name="CustomWindowTitleBackground">
       	<item name="android:background">#565656</item>
	</style>

	<style name="vs_main_activity_style" parent="android:Theme">
	       <item name="android:windowBackground">@color/vs_transparent</item>  
           <item name="android:windowIsTranslucent">true</item>  
           <item name="android:windowNoTitle">true</item>       
           <item name="android:windowFullscreen">false</item>
           <item name="android:windowTranslucentStatus">true</item>
	</style>
	
	 <style name="vs_shopRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:maxHeight">10.5dip</item>
        <item name="android:progressDrawable">@drawable/vs_rating_star_style</item>
        <item name="android:minHeight">10.5dip</item>
        <item name="android:isIndicator">true</item>
    </style>
    
	  <style name="vs_flight_number_title">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/vs_black</item>
        <item name="android:textStyle">bold</item>
<!--         shigq add name="android:textStyle">bold-->
<!--         <item name="android:textSize">16.0sp</item> -->
<!-- 		<item name="android:textColor">@color/vs_news_content</item> -->
    </style>
    
	  <style name="vs_flight_number_table_actical_time">
        <item name="android:textSize">12.0sp</item>
<!--         shigq -->
<!--         <item name="android:textColor">#ff468cc7</item> -->
        <item name="android:textColor">#ff3da0fd</item>
    </style>
    
    <style name="vs_train_type_divider">
        <item name="android:background">@drawable/vs_interest_divider_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0dip</item>
        <item name="android:layout_marginLeft">3.0dip</item>
        <item name="android:layout_marginRight">3.0dip</item>
    </style>
    <style name="vs_flight_number_table_root">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffa8a8a8</item>
    </style>
    
    <style name="vs_train_s2s_station">
        <item name="android:textSize">17.0sp</item>
<!--         shigq -->
<!--         <item name="android:textColor">@color/vs_news_content</item> -->
		<item name="android:textColor">@color/vs_black</item>
		
    </style>
    
    <style name="vs_train_s2s_divider">
<!--         <item name="android:background">#ffe2e2e2</item> -->
        <item name="android:layout_width">fill_parent</item>
<!--         <item name="android:layout_height">0.6dip</item> -->
<!--         shigq -->
        <item name="android:layout_height">0.3dip</item>
        <item name="android:background">#ffcfcfcf</item>
    </style>
    
     <style name="vs_train_time_text">
        <item name="android:textSize">14.0sp</item>
<!--         shigq -->
<!--         <item name="android:textColor">#ff468cc7</item> -->
        <item name="android:textColor">#ff3da0fd</item>
    </style>
    
     <style name="Theme.Translucent" parent="android:style/Theme.Translucent">
        <item name="android:windowBackground">@drawable/vs_speed_bg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
    
