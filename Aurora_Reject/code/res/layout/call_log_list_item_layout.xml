<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@com.aurora:dimen/aurora_list_doubleline_height"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="13.6dp"
        android:layout_marginRight="17dp"
        android:orientation="horizontal" >

        <aurora.widget.AuroraCheckBox
            android:id="@+id/check_box"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="17dp"
            android:focusable="false"
            android:clickable="false"
            android:focusableInTouchMode="false" />

        <aurora.widget.AuroraTextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:singleLine="true"
            android:textColor="#FF000000"
            android:textSize="@com.aurora:dimen/aurora_list_singleline_text_size" />
    </LinearLayout>

    <aurora.widget.AuroraTextView
        android:id="@+id/number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="17dp"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="5dp"
        android:singleLine="true"
        android:textColor="#b5b5b5"
        android:textSize="@com.aurora:dimen/aurora_list_subtext_text_size" />

</LinearLayout>