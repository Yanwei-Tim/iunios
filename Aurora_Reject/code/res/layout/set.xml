    </aurora.preference.AuroraPreferenceCategory>
    
     <aurora.preference.AuroraPreferenceCategory
        aurora:aurorakey="pref_key_phone_settings"
        aurora:auroratitle="" >
        <aurora.preference.AuroraPreference
            aurora:aurorakey="mark"
            aurora:aurorasummary=""
            aurora:auroratitle="@string/mark_manage" />
